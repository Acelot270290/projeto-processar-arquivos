{
    "openapi": "3.0.0",
    "info": {
        "title": "Desafio Fullstack API",
        "description": "Documentação da API de upload e processamento de arquivos",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:9584",
            "description": "Servidor local"
        }
    ],
    "paths": {
        "/api/processed-records": {
            "post": {
                "tags": [
                    "Processed Records"
                ],
                "summary": "Faz upload de um arquivo e salva os registros processados",
                "description": "Upload e processamento de arquivo",
                "operationId": "21bd31c908c112970345b6ae61bfcf2c",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "file"
                                ],
                                "properties": {
                                    "file": {
                                        "description": "Arquivo a ser processado",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Arquivo processado com sucesso"
                    },
                    "422": {
                        "description": "Erro de validação"
                    }
                }
            }
        },
        "/api/files": {
            "get": {
                "tags": [
                    "Processed Records"
                ],
                "summary": "Retorna os nomes dos arquivos processados",
                "description": "Lista os nomes de arquivos já processados",
                "operationId": "4c0e22c190cf94ec833543aada5330f1",
                "responses": {
                    "200": {
                        "description": "Lista de nomes de arquivos",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string",
                                        "example": "exemplo_dados.csv"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/files/{id}": {
            "get": {
                "tags": [
                    "Processed Records"
                ],
                "summary": "Retorna os registros processados de um arquivo",
                "description": "Retorna os registros processados de um arquivo específico",
                "operationId": "09d16265342087e9c2bdd1941c5e1973",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Nome do arquivo",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "exemplo_dados.csv"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Registros do arquivo",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "data_registro": {
                                                "type": "string",
                                                "format": "date"
                                            },
                                            "metrica_a": {
                                                "type": "number",
                                                "format": "float"
                                            },
                                            "metrica_b": {
                                                "type": "number",
                                                "format": "float"
                                            },
                                            "indicador_x": {
                                                "type": "number",
                                                "format": "float"
                                            },
                                            "indicador_y": {
                                                "type": "number",
                                                "format": "float"
                                            },
                                            "id": {
                                                "type": "string"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Processed Records",
            "description": "Processed Records"
        }
    ]
}